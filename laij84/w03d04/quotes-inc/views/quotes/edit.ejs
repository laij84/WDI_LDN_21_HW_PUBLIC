<form action="/quotes/<%= quoteId.id%>" method="POST">
<input type="hidden" name="_method" value="PUT">
  <div class="form-group">
    <input type="text" name="quote[quote]" placeholder="This is a quote..." class="form-control" value="<%= quoteId.quote %>">
  </div>

  <div class="form-group">
    <input type="text" name="quote[author]" placeholder="Maxwell P. Strong" class="form-control" value="<%= quoteId.author %>">
  </div>

  <div class="form-group">
    <input type="submit" value='Edit Quote' class="btn btn-default">
  </div>
</form>